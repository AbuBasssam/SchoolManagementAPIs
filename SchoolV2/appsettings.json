{



  "ConnectionStrings": {
    "DefaultConnection": "Server={Your_Server}; Database={Your_DATABASE}; User Id=your ID; Password=YourPassword; Encrypt=False; TrustServerCertificate=True; Connection Timeout=30;"
  },

  "JwtSettings": {
    "Secret": "[Your_Secert_Key}",
    "Issuer": "{SchoolAPIProject",
    "Audience": "MobileApp",
    "ValidateAudience": true,
    "ValidateIssuer": true,
    "ValidateLifeTime": true,
    "ValidateIssuerSigningKey": true,
    "AccessTokenExpireDate": 1,
    "RefreshTokenExpireDate": 20
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
